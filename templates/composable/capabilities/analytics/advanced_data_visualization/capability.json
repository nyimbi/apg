  "features": [
    "Interactive Charts",
    "3D Visualizations",
    "Geographic Maps",
    "Network Graphs",
    "Animated Charts",
    "Custom Widgets",
    "Export Capabilities",
    "Responsive Design"
  ],
  "configuration": {},
  "compatible_bases": [
    "flask_webapp",
    "dashboard"
  ],
  "incompatible_capabilities": []
}