{
  "name": "Advanced Data Visualization",
  "category": "analytics",
  "description": "Advanced interactive data visualizations and charts",
  "version": "1.0.0",
  "author": "APG Team",
  "python_requirements": [
    "plotly>=5.15.0",
    "bokeh>=3.2.0",
    "altair>=5.0.0",
    "matplotlib>=3.7.0",
    "seaborn>=0.12.0",
    "d3py>=0.2.8"
  ],
  "system_requirements": [],
  "dependencies": [
    {
      "name": "data/postgresql_database",
      "version": null,
      "optional": false,
      "reason": "Data source for visualizations"
    }
  ],
  "integration": {
    "models": [
      "Visualization",
      "Chart",
      "Dataset"
    ],
    "views": [
      "VisualizationView",
      "ChartBuilderView"
    ],
    "apis": [
      "viz/create",
      "viz/data",
      "viz/export"
    ],
    "templates": [
      "chart_builder.html",
