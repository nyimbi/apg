{
  "name": "ML Model Training",
  "category": "ai",
  "description": "Machine learning model training, evaluation, and deployment pipeline",
  "version": "1.0.0",
  "author": "APG Team",
  "python_requirements": [
    "scikit-learn>=1.3.0",
    "pandas>=2.0.0",
    "numpy>=1.24.0",
    "joblib>=1.3.0",
    "mlflow>=2.5.0",
    "optuna>=3.2.0",
    "xgboost>=1.7.0"
  ],
  "system_requirements": [],
  "dependencies": [
    {
      "name": "data/postgresql_database",
      "version": null,
      "optional": false,
      "reason": "Store training data and model metadata"
    },
    {
      "name": "analytics/basic_analytics",
      "version": null,
      "optional": false,
      "reason": "Model performance visualization"
    }
  ],
  "integration": {
    "models": [
      "MLModel",
      "TrainingJob",
      "Dataset",
      "Experiment",
      "ModelVersion"
    ],
    "views": [
      "ModelView",
      "TrainingView",
      "ExperimentView"
    ],
    "apis": [
      "ml/train",
      "ml/predict",
      "ml/evaluate",
      "ml/deploy"
    ],
    "templates": [
      "ml_dashboard.html",
      "training_monitor.html"
    ],
    "static_files": [],
    "config_additions": {
      "ML_MODEL_STORAGE": "/var/models",
      "MLFLOW_TRACKING_URI": "sqlite:///mlflow.db"
    },
    "database_migrations": []
  },
  "features": [
    "Automated ML Pipeline",
    "Model Training",
    "Hyperparameter Optimization",
    "Cross-validation",
    "Model Evaluation",
    "Model Versioning",
    "A/B Testing",
    "Performance Monitoring"
  ],
  "configuration": {},
  "compatible_bases": [
    "flask_webapp",
    "microservice"
  ],
  "incompatible_capabilities": []
}