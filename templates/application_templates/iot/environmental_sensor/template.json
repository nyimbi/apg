{
  "template_id": "iot/environmental_sensor",
  "name": "Environmental Monitoring",
  "description": "Environmental sensor network with data analysis",
  "complexity": "Intermediate",
  "domain": "Environmental Monitoring",
  "version": "1.0.0",
  "apg_version": ">=1.0.0",
  "agents": [
    "SensorAgent",
    "AnalyticsAgent",
    "PredictionAgent"
  ],
  "digital_twins": [
    "SensorNetworkTwin",
    "EnvironmentTwin"
  ],
  "features": [
    "Multi-sensor Integration",
    "Environmental Analytics",
    "Trend Prediction"
  ],
  "databases": [
    "sensor_readings",
    "environmental_data",
    "predictions"
  ],
  "requirements": [
    "Flask-AppBuilder>=4.3.0",
    "Flask>=2.3.0",
    "SQLAlchemy>=2.0.0"
  ],
  "variables": {
    "project_name": "{{project_name}}",
    "project_description": "{{project_description}}",
    "author": "{{author}}",
    "database_url": "{{database_url}}",
    "secret_key": "{{secret_key}}"
  },
  "files": [
    "app.py.template",
    "config.py.template",
    "requirements.txt.template",
    "README.md.template"
  ]
}