{
  "template_id": "basic/dashboard",
  "name": "Analytics Dashboard",
  "description": "Real-time analytics dashboard with charts and metrics",
  "complexity": "Intermediate",
  "domain": "Analytics",
  "version": "1.0.0",
  "apg_version": ">=1.0.0",
  "agents": [
    "AnalyticsAgent",
    "MetricsCollector"
  ],
  "digital_twins": [],
  "features": [
    "Real-time Charts",
    "KPI Tracking",
    "Data Visualization"
  ],
  "databases": [
    "analytics_data",
    "metrics"
  ],
  "requirements": [
    "Flask-AppBuilder>=4.3.0",
    "Flask>=2.3.0",
    "SQLAlchemy>=2.0.0"
  ],
  "variables": {
    "project_name": "{{project_name}}",
    "project_description": "{{project_description}}",
    "author": "{{author}}",
    "database_url": "{{database_url}}",
    "secret_key": "{{secret_key}}"
  },
  "files": [
    "app.py.template",
    "config.py.template",
    "requirements.txt.template",
    "README.md.template"
  ]
}